<h1>Listing contact_forms</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Email</th>
    <th>Subject</th>
    <th>Description</th>
    <th></th>
    <th></th>
  </tr>

<% @contact_forms.each do |contact_form| %>
  <tr>
    <td><%= contact_form.name %></td>
    <td><%= contact_form.email %></td>
    <td><%= contact_form.subject %></td>
    <td><%= contact_form.description %></td>
    <td><%= link_to 'Show', admin_contact_form %></td>
    <td><%= link_to 'Destroy', admin_contact_form, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />
